<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/web-app"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/web-app 
         https://jakarta.ee/xml/web-app/web-app_6_0.xsd"
         version="6.0">
    
    <!-- Remove any duplicate servlet definitions -->
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>com.master.movieapp.controller.HomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
</web-app>